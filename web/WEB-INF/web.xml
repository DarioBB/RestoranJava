<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <context-param>
    <description>username za testiranje</description>
    <param-name>uname</param-name>
    <param-value>pero</param-value>
  </context-param>
  <context-param>
    <description>password za testiranje</description>
    <param-name>passwd</param-name>
    <param-value>peric</param-value>
  </context-param>
  <servlet>
    <servlet-name>SessionServlet</servlet-name>
    <servlet-class>restoran.session.SessionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SessionServlet</servlet-name>
    <url-pattern>/Session</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>20</session-timeout>
  </session-config>
</web-app>